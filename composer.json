{
    "name": "wearejh/hub",
    "description": "ZF2 Container App for Other modules",
    "license": "MIT",
    "authors": [
        {
            "name": "Aydin Hassan",
            "email": "aydin@hotmail.co.uk"
        }
    ],
    "require": {
        "php": ">=5.4",
        "zendframework/zendframework": "~2.3.0",
        "doctrine/doctrine-module": "dev-master as 0.8.0",
        "doctrine/doctrine-orm-module": "0.*",
        "bjyoungblood/bjy-authorize": "1.4.*",
        "beberlei/DoctrineExtensions": "dev-master",
        "doctrine/data-fixtures": "1.0.*",
        "zf-commons/zfc-admin": "0.1.*",
        "rwoverdijk/assetmanager": "1.3.*",
        "hybridauth/hybridauth": "dev-master",
        "wearejh/jh-user": "dev-master",
        "wearejh/flexitime": "dev-installer",
        "wearejh/installer": "dev-master"
    },
    "require-dev": {
        "zendframework/zftool": "dev-master",
        "zendframework/zend-developer-tools": "dev-master",
        "phpunit/phpunit": "4.0.*",
        "satooshi/php-coveralls": "dev-master"
    },
    "repositories": [
        {
            "type": "vcs",
            "url" : "https://github.com/WeareJH/Flexitime.git"

        },
        {
            "type": "vcs",
            "url" : "https://github.com/WeareJH/JhUser.git"

        },
        {
            "type": "vcs",
            "url" : "https://github.com/WeareJH/Installer.git"

        }
    ],
    "scripts": {
        "post-update-cmd": "php public/index.php install"
    }
}
